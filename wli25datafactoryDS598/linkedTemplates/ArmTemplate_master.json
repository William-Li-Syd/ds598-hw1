{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "wli25datafactoryDS598"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"serpapi_finance_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'serpapi_finance'"
		},
		"wli25github_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'wli25github'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://wli25storage.dfs.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://wli25-key-vault.vault.azure.net/"
		},
		"open_weather_air_pollution_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://api.openweathermap.org/"
		},
		"open_weather_air_pollution_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "liw25"
		},
		"open_weather_rest_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://history.openweathermap.org/"
		},
		"open_weather_rest_api_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "liw25"
		},
		"serpapi_google_finance_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://serpapi.com/search?engine=google_finance"
		},
		"wli25github_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "William-Li-Syd"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "wli25datafactoryDS598_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"serpapi_finance_connectionString": {
						"value": "[parameters('serpapi_finance_connectionString')]"
					},
					"wli25github_credential": {
						"value": "[parameters('wli25github_credential')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"serpapi_google_finance_properties_typeProperties_url": {
						"value": "[parameters('serpapi_google_finance_properties_typeProperties_url')]"
					},
					"wli25github_properties_typeProperties_username": {
						"value": "[parameters('wli25github_properties_typeProperties_username')]"
					},
					"open_weather_air_pollution_properties_typeProperties_url": {
						"value": "[parameters('open_weather_air_pollution_properties_typeProperties_url')]"
					},
					"open_weather_air_pollution_properties_typeProperties_userName": {
						"value": "[parameters('open_weather_air_pollution_properties_typeProperties_userName')]"
					},
					"open_weather_rest_api_properties_typeProperties_url": {
						"value": "[parameters('open_weather_rest_api_properties_typeProperties_url')]"
					},
					"open_weather_rest_api_properties_typeProperties_userName": {
						"value": "[parameters('open_weather_rest_api_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "wli25datafactoryDS598_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'wli25datafactoryDS598_ArmTemplate_0')]"
			]
		}
	]
}